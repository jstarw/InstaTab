{
  "manifest_version": 2,

    "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "name": "Instatab",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }

}